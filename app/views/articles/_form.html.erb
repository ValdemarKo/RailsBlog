<%= form_with model: article do |form|%>
<div><%= form.label :title%><br>
<%= form.text_field :title%>
</div>


<div><%= form.label :body%><br>
<%= form.text_area :body%>
</div>
<div><%= form.label :author%><br>
<%= form.text_field :author%>
</div>
<div style="margin-top:10px"><%= form.submit %>
</div>
<%end%> 